        "import time\n",
        "# INITIATING THE MODEL\n",
        "model = gai.GenerativeModel('gemini-pro')\n",
        "\n",
        "# GENERATING THE CHAT SESSION\n",
        "chat = model.start_chat(history=[])\n",
        "\n",
        "# AN INFINITE LOOP FOR ON GOING CHAT\n",
        "while True:\n",
        "    prompt = input('User: ')\n",
        "    # Setting the exit logic for the Chat Loop\n",
        "    if prompt.lower() not in ['exit', 'quit', 'bye']:\n",
        "        # Sending the Chat Prompt to the API\n",
        "        response = chat.send_message(prompt)\n",
        "\n",
        "        # Getting the last item from the chat history (the AI Answer)\n",
        "        print(f\"{chat.history[-1].role.capitalize()}: {chat.history[-1].parts[0].text}\")\n",
        "        print('\\n' + '-' * 100 + '\\n')\n",
        "    else:\n",
        "        print('Exiting...')\n",
        "        time.sleep(2)\n",
        "        print('See ya ...')\n",
        "        break"
